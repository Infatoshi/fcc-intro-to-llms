{
  "enabled": true,
  "embeddingProvider": "gemini",
  "embeddingModel": "gemini-embedding-001",
  "searchThreshold": 0.4,
  "maxResults": 5,
  "chunkingStrategy": "logical",
  "maxFileSizeKB": 500,
  "maxDirectorySizeMB": 50,
  "maxFiles": 1000,
  "includePatterns": [
    "**/*.ts",
    "**/*.js",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.py",
    "**/*.java",
    "**/*.cpp",
    "**/*.c",
    "**/*.h",
    "**/*.go",
    "**/*.rs",
    "**/*.php",
    "**/*.rb",
    "**/*.swift",
    "**/*.kt",
    "**/*.cs",
    "**/*.scala",
    "**/*.clj",
    "**/*.sh",
    "**/*.yml",
    "**/*.yaml",
    "**/*.json",
    "**/*.md",
    "**/*.txt"
  ],
  "excludePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.git/**",
    "**/coverage/**",
    "**/target/**",
    "**/bin/**",
    "**/obj/**",
    "**/.next/**",
    "**/.nuxt/**",
    "**/vendor/**",
    "**/__pycache__/**",
    "**/*.min.js",
    "**/*.min.css",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/pnpm-lock.yaml"
  ]
}